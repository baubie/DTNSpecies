size 8 6
set font psh
set hei 0.15
set lwidth 0.01
begin graph
    size 8 6
    scale auto
    xtitle "Stimulus Duration (ms)"
    ytitle "Mean Spikes per Trial"
    x2axis off
    y2axis off
    yaxis min 0
    xaxis min 0 max 1000.0
    data "c_nmda_mean.dat"
    xticks length -0.05
    yticks length -0.05
    d1 line color rgb(1,0,0)
    d3 line color rgb(0,0,1)
    d5 line color rgb(0,1,0)
    key compact nobox
end graph
